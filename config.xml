<?xml version="1.0" encoding="UTF-8"?>
<widget id="com.au" version="1.0.0" android-versionCode="1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Au</name>
  <description>Built with NEXUS APK Builder</description>
  <author>NEXUS APK User</author>
  <content src="index.html" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <preference name="DisallowOverscroll" value="true" />
  <preference name="android-minSdkVersion" value="24" />
  <preference name="android-targetSdkVersion" value="34" />
  <preference name="Fullscreen" value="false" />
  <preference name="Orientation" value="default" />
  <preference name="AndroidPersistentFileLocation" value="Internal" />
  <platform name="android">
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon.png" />
    <icon density="mdpi" src="res/icon.png" />
    <icon density="hdpi" src="res/icon.png" />
    <icon density="xhdpi" src="res/icon.png" />
    <icon density="xxhdpi" src="res/icon.png" />
    <icon density="xxxhdpi" src="res/icon.png" />
    <plugin name="cordova-plugin-network-information" spec="^4.0.0" />
    <plugin name="cordova-plugin-media" spec="^4.0.0" />
    <plugin name="cordova-plugin-file" spec="^4.0.0" />
  </platform>
</widget>